{
  "name": "vue-ghpages-blog",
  "version": "1.1.0",
  "description": "A blog based on GitHub Pages by Vue.js 2 + Webpack 2",
  "main": "src/main.js",
  "scripts": {
    "dev": "webpack-dev-server --inline --hot",
    "prebuild": "rimraf dist/*",
    "build": "cross-env NODE_ENV=production webpack -p --progress --hide-modules",
    "lint": "eslint --ext \".js, .vue\" src",
    "pretest": "npm run lint",
    "test": "karma start test/unit/karma.conf.js --single-run"
  },
  "engines": {
    "node": ">=4",
    "npm": ">=3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/viko16/vue-ghpages-blog.git"
  },
  "keywords": [
    "blog",
    "github",
    "vuejs",
    "webpack"
  ],
  "author": "viko16 (https://github.com/viko16)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/viko16/vue-ghpages-blog/issues"
  },
  "homepage": "https://github.com/viko16/vue-ghpages-blog#readme",
  "devDependencies": {
    "babel-core": "^6.0.0",
    "babel-loader": "^6.0.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-stage-1": "^6.13.0",
    "chai": "^3.5.0",
    "cross-env": "^3.1.0",
    "css-loader": "^0.25.0",
    "eslint": "^3.7.0",
    "eslint-config-standard": "^6.0.0",
    "eslint-loader": "^1.5.0",
    "eslint-plugin-html": "^1.5.3",
    "eslint-plugin-promise": "^3.3.0",
    "eslint-plugin-standard": "^2.0.0",
    "extract-text-webpack-plugin": "^2.0.0-beta.4",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.22.0",
    "inject-loader": "^3.0.0-beta2",
    "json-loader": "^0.5.4",
    "karma": "^1.3.0",
    "karma-mocha": "^1.2.0",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-sinon-chai": "^1.2.4",
    "karma-spec-reporter": "^0.0.26",
    "karma-webpack": "^1.8.0",
    "mocha": "^3.1.2",
    "rimraf": "^2.5.4",
    "sinon": "^1.17.6",
    "sinon-chai": "^2.8.0",
    "style-loader": "^0.13.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^2.3.1",
    "vue-hot-reload-api": "^2.0.0",
    "vue-html-loader": "^1.2.3",
    "vue-loader": "^10.0.0",
    "vue-style-loader": "^1.0.0",
    "vue-template-compiler": "^2.1.0",
    "webpack": "2.1.0-beta.27",
    "webpack-dev-server": "2.1.0-beta.11"
  },
  "dependencies": {
    "axios": "^0.15.2",
    "es6-promise": "^4.0.0",
    "front-matter": "^2.1.0",
    "marked": "^0.3.6",
    "normalize.css": "^5.0.0",
    "prismjs": "^1.5.1",
    "timeago.js": "^2.0.2",
    "vue": "^2.1.0",
    "vue-router": "^2.0.0"
  }
}
